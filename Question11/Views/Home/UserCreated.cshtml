@model Question11.Models.User;
@{
    ViewData["Title"] = "User Profile";
    <h2>A new user is created with the following details</h2>
    <p>Name: @ViewBag.details.Name</p>
    <p>Id: @ViewBag.details.Id</p>
    <p>Email: @ViewBag.details.EmailId</p>
    <p>Role: @ViewBag.details.Role</p>

}
